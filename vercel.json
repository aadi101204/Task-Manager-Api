{
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist",
    "functions": {
        "api/main.py": {
            "runtime": "python3.9"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}